#Вар 8, методичка 1, задание 1.1 с помощью double
#include <iostream>
#include <math.h>
using namespace std;
int main()
{
    double a, b, c, d, f;
    cin >> a;
    cin >> b;
    c = pow((a+b), 4);
    d = pow(a, 4) + (4*a*a*a*b) + (6*a*a*b*b); 
    f = (4*a*b*b*b) + pow(b, 4);
    cout << "\n" << ((c-d)/f);
    return 0;
}
